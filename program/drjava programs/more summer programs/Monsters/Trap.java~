import java.awt.*;
  public class Trap{
    private int xPos,yPos;
    private int damage;
    
    public Trap(int xPos, int yPos, int damage){
    super();
    this.xPos = xPos;
    this.yPos = yPos;
    this.damage = damage
    }
    
    public int getXPos(){
      return xPos;
    }
    public int getYPos(){
     return yPos; 
    }
    public boolean getDamage(){
      return damage;
    }
    public void setDamage(int damage){
      this.damage=damage;
    }
  }
    