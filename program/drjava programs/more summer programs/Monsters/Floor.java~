  import java.awt.*;
  public class Floor{
    private int xPos,yPos;
    
    public Floor(int xPos, int yPos){
    super();
    this.xPos = xPos;
    this.yPos = yPos;
    }
    
    public int getXPos(){
      return xPos;
    }
    public int getYPos(){
     return yPos; 
    }
      
  }